<% layout('layouts/boilerplate')%>
    <style>
        body {
            background-image: url("https://media.istockphoto.com/photos/background-black-total-grunge-abstract-cement-concrete-paper-texture-picture-id1251205256?b=1&k=20&m=1251205256&s=170667a&w=0&h=o4N-O5lZywE2lBuauTZX6NiutPCNbKAnuKaTSKWM0Lo=");
            background-color: black;
        }
    </style>

    <div class="conatiner">
        <div class="row">
            <div class="col-md-12 mt-2">
                <H1 class="text-center" style="color: white;">YOUR GENERATED QR</H1>
            </div>
        </div>
        <div class="row mt-4">
            <div class="co-md-12">
                <div class="text-center">
                    <h2 class="text-center" style="color: white;">Click to Download below</h2>
                </div>
            </div>

            <div class="d-flex justify-content-center">
                <a download="qr.jpg" href="<%=url%>" title="ImageName" class="btn btn-success">Download</a>
            </div>
        </div>

        <div class="row">
            <div class="d-flex justify-content-center">
                <div>
                    <a download="qr.jpg" href="<%=url%>" title="ImageName">

                        <img alt="ImageName" src="<%=url%>" style="height:300px">

                    </a><br>
                </div>

            </div>
        </div>

    </div>